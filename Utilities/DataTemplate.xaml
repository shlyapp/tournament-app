<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TournamentApp.ViewModel"
                    xmlns:view="clr-namespace:TournamentApp.View">

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TableViewModel}">
        <view:Table />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TournamentGridViewModel}">
        <view:TournamentGrid />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:CalculatorViewModel}">
        <view:CalculatorView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CrateTurnamentViewModel}">
        <view:CrateTurnament />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <view:SettingsView />
    </DataTemplate>

</ResourceDictionary>